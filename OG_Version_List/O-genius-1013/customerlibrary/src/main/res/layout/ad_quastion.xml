<?xml version="1.0" encoding="utf-8"?>
<com.orange.jzchi.jzframework.Widget.AspetRelate xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="1dp"
        android:layout_height="match_parent"
        app:srcCompat="@color/background" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        app:srcCompat="@color/background" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:srcCompat="@color/background" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        app:srcCompat="@color/background" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/textView3"
        android:layout_marginTop="10dp"
        app:srcCompat="@mipmap/app_btn_bluetooth_n" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="5dp"
        android:gravity="center_horizontal"
        android:padding="5dp"
        android:text="Bluetooth connection"
        android:textColor="@color/Black" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="More......."
        android:textColor="@color/Black" />
</com.orange.jzchi.jzframework.Widget.AspetRelate>