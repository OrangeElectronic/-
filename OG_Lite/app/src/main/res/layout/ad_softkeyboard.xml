<?xml version="1.0" encoding="utf-8"?>
<com.orange.jzchi.jzframework.Widget.AspetRelate xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:ratio="6.5"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/output"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.orange.og_lite.Util.Util_ClearEditText
            android:id="@+id/ID"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="0.5"
            android:digits="abcdefABCDEF1234567890"
            android:enabled="true"
            android:focusable="auto"
            android:focusableInTouchMode="true"
            android:gravity="center"
            android:inputType="textVisiblePassword"
            android:textColor="@color/text_black"
            android:textSize="18sp" />

        <Button
            android:id="@+id/delete"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="?attr/actionModeCloseDrawable" />

        <Button
            android:id="@+id/enter"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:text="jz.192" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/input"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible">

        <Button
            android:id="@+id/button"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:text="Button" />

        <Button
            android:id="@+id/button2"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:text="Button" />

        <Button
            android:id="@+id/button3"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:text="Button" />

        <Button
            android:id="@+id/button12"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:text="Button" />

    </LinearLayout>

</com.orange.jzchi.jzframework.Widget.AspetRelate>